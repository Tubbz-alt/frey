{
  "name": "frey",
  "version": "0.3.42",
  "description": "Automated infrastructure CLI",
  "scripts": {
    "acceptance:savefixtures": "source env.sh && env SAVE_FIXTURES=true npm run acceptance",
    "acceptance": "npm run build && ./test/acceptance.sh ${SCENARIO}",
    "babel": "babel src --out-dir lib --source-maps",
    "build:watch": "babel src --watch --source-maps --out-dir lib",
    "build": "npm run babel && npm run libversion",
    "changes": "git log --pretty=format:\"- %s%n%b\" --since=\"${TAG:-$(git show -s --format=%ad `git rev-list --tags --max-count=1`)}\"",
    "encrypt": "source env.sh && bash scripts/encrypt.sh",
    "fix": "eslint . --fix",
    "libversion": "node ./lib/cli.js --version",
    "lint:sh": "shellcheck **/*.sh **.sh",
    "lint": "eslint .",
    "mocha:coverage": "npm run mocha && istanbul report text-summary lcov && (cat coverage/lcov.info | coveralls || true)",
    "mocha": "mocha --compilers js:babel-register --grep=\"${GREP}\" --recursive test${MOCHA_TRAILER} src/**.test.js",
    "release:major": "env SEMANTIC=major npm run release",
    "release:minor": "env SEMANTIC=minor npm run release",
    "release:patch": "env SEMANTIC=patch npm run release",
    "release": "npm version ${SEMANTIC:-patch} -m \"Release %s\" && npm run version:replace && git commit test/fixture/*.stdio -m 'Update version' && npm run test && git push --tags && git push && npm publish",
    "test:full": "npm run lint && npm run build && npm run mocha:coverage && npm run acceptance",
    "test": "npm run lint && npm run build && npm run mocha",
    "toc": "doctoc --github --notitle --maxlevel 3 README.md",
    "version:current": "node -e 'console.log(require(\"./package.json\").version)'",
    "version:replace": "replace-x \"Frey version '\\d+\\.\\d+\\.\\d+'\" \"Frey version '$(npm run --silent version:current)'\" test/fixture/*.stdio"
  },
  "files": [
    "bin",
    "roles",
    "lib"
  ],
  "main": "lib/Frey.js",
  "bin": {
    "frey": "lib/cli.js"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/freyproject/frey.git"
  },
  "dependencies": {
    "async": "1.5.2",
    "babel-polyfill": "6.13.0",
    "chalk": "1.1.3",
    "depurar": "0.3.0",
    "es6-promise": "^4",
    "fakefile": "0.0.8",
    "flat": "2.0.0",
    "globby": "4.0.0",
    "grunt": "0.4.5",
    "indent-string": "2.1.0",
    "inflection": "1.8.0",
    "ini": "1.3.4",
    "inquirer": "0.12.0",
    "js-yaml": "3.5.5",
    "liftoff": "2.2.1",
    "lodash": "4.6.1",
    "mkdirp": "0.5.1",
    "os-homedir": "1.0.1",
    "rc": "1.1.6",
    "replace-x": "1.5.0",
    "scrolex": "0.0.27",
    "semver": "5.1.0",
    "strip-indent": "1.0.1",
    "toml": "2.3.0",
    "tomlify-j0.4": "1.0.1",
    "update-notifier": "0.6.3",
    "uuid": "3.0.0",
    "yargs": "4.3.2"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-es6-promise": "1.1.1",
    "babel-plugin-syntax-async-functions": "6.13.0",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-register": "6.14.0",
    "browser-sync": "2.11.0",
    "chai": "3.5.0",
    "coveralls": "2.11.9",
    "cross-env": "1.0.7",
    "doctoc": "1.0.0",
    "eslint": "3.15.0",
    "eslint-config-standard": "6.2.1",
    "eslint-plugin-promise": "3.4.1",
    "eslint-plugin-standard": "2.0.1",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "node-sass": "3.11.1",
    "nodemon": "1.8.1",
    "npm-run-all": "2.1.1",
    "should": "8.3.0"
  },
  "keywords": [
    "terraform",
    "ansible",
    "infra",
    "automation"
  ],
  "author": "Kevin van Zonneveld",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/freyproject/frey/issues"
  },
  "homepage": "https://github.com/freyproject/frey#readme"
}
